{
  "name": "y",
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/setuptests.js",
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "scriptPreprocessor": "<rootDir>/node_modules/jest-css-modules",
    "collectCoverageFrom": [
      "client/**/*.{jsx}",
      "!**/node_modules/**"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "version": "1.0.0",
  "description": "Reviews module for AirCnB",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "dev-react": "webpack -d --watch",
    "start": "nodemon server/index.js",
    "eslint": "eslint . --ext .jsx --ext .js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AirCnB/reviews-module.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AirCnB/reviews-module/issues"
  },
  "homepage": "https://github.com/AirCnB/reviews-module#readme",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-preset-airbnb": "^2.5.2",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "express": "^4.16.3",
    "identity-obj-proxy": "^3.0.0",
    "jest-enzyme": "^6.0.2",
    "mongoose": "^5.2.4",
    "save-dev": "^2.0.0",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.0.8"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^1.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "jest": "^23.4.1",
    "jest-css-modules": "^1.1.0",
    "lorem-hipsum": "^0.1.7",
    "lorem-ipsum": "^1.0.5",
    "nodemon": "^1.18.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "style-loader": "^0.21.0",
    "supertest": "^3.1.0"
  },
  "directories": {
    "test": "tests"
  }
}
